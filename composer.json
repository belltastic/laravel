{
    "name": "belltastic/laravel",
    "description": "Laravel helper package to easily send Belltastic Notifications",
    "keywords": [
        "belltastic",
        "laravel",
        "notifications"
    ],
    "homepage": "https://github.com/belltastic/laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Belltastic",
            "email": "info@belltastic.com",
            "role": "Owner"
        },
        {
            "name": "Arunas Skirius",
            "email": "arunas@belltastic.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.73"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Belltastic\\": "src",
            "Belltastic\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Belltastic\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Belltastic\\BelltasticServiceProvider"
            ],
            "aliases": {
                "Belltastic": "Belltastic\\Facades\\Belltastic"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
